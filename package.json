{
    "name": "firemodel-syntax",
    "displayName": "FireModel Support",
    "description": "FireModel syntax highlighting",
    "version": "0.0.1",
    "publisher": "ozouai",
    "license": "MIT",
    "repository": {
        "url": "https://github.com/ozouai/firemodel-vscode"
    },
    "engines": {
        "vscode": "^1.27.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "firemodel",
            "aliases": ["FireModel", "firemodel"],
            "extensions": [".firemodel"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "firemodel",
            "scopeName": "source.firemodel",
            "path": "./syntaxes/firemodel.tmLanguage.json"
        }]
    }
}